<div class="webmap-name">{{mapName}}</div>

<div class="builder-config-section larger">
  <div class="section-content button">
    <div class="config-item button btn btn-bw" data-action="swap">
      <div class="edit-media-icons">
        <i class="fa fa-image icon-image"></i>
        <i class="fa fa-globe icon-globe"></i>
        <i class="fa fa-map icon-map"></i>
        {{strings.changeMedia}}
      </div>
    </div>
  </div>
</div>
<div class="builder-config-section larger">
  <div class="section-content button">
    <div class="config-item button btn btn-bw{{#if showErrors}} disabled{{/if}}" data-action="edit">
      {{#if mapEditorEnabled}}
        <span class="config-icon fa-stack">
          <i class="fa fa-map-o"></i>
          <i class="fa fa-pencil overlaid-icon"></i>
        </span>
      {{else}}
        <i class="config-icon fa fa-external-link"></i>
      {{/if}}
      {{strings.edit}}
    </div>
  </div>
  {{#unless mapEditorEnabled}}
    <span class="aside">{{strings.editAside}}</span>
  {{/unless}}
</div>
<div class="builder-config-section">
  <div class="section-content button">
    <div class="config-item button btn btn-bw" data-action="remove">
      <i class="config-icon fa fa-trash-o"></i>
      {{strings.remove}}
    </div>
  </div>
</div>
